programa(){
	entero x := -1;
	entero tam := 10;
	entero vec[tam];
	booleano band := false;

	if(x < 0){
		entero y := x + -2;
		
		if(y <= 3 Y y > 0){
			x := 36;
		}
		else{
			vec[x + 4/2] := y + 10 + 1;
			avanzar((y * x / 4 + 1 ));
		}
	}

	do
		//y := 43;

		entero i;
		for(i := 0 ; i := i + 1 ; i < 10){
			avanzar(x+2);
			x := x*3;
			if(x mod 2 = 0){
				mirarArriba();
			}else{
				mirarAbajo();
			}
			vec[i] := x * (x+1) / 2;

			if(x > 7){
				band := !band;
			}
		}
	while(band);
}